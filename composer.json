{
    "name": "atmosmaciel/api-with-silex",
    "description": "api-with-silex",
    "minimum-stability": "dev",
    "authors":[
       {
           "name": "Atmos Maciel",
           "email": "atmosmaciel@heptum.com.br"
       }
    ],
    "require": {
    	"silex/silex": "^2.0",
        "doctrine/common": "2.4.*",
        "doctrine/dbal": "2.4.*",
        "doctrine/orm": "2.4.*",
        "dflydev/doctrine-orm-service-provider": "2.*", /** provider pronto para se usar o ORM Doctrine **/
        "jms/serializer": "^1.3" /** vai servir para serializar os nossos objetos para que nosso controller possa ler **/
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
    	"psr-4": {
    		"Api\\":"src/"
    	}
    }
}
